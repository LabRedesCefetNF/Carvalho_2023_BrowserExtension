{
    "manifest_version":3,
    "name":"Tcc",
    "version":"1.0",
    "description":"Protótipo tcc",
    "icons": {
        "48":"icon48.png",
        "128":"icon128.png"
    },
    "options_page":"src/option.html",
    "action":{
        "default_icon": {
            "19":"icon19.png",
            "38":"icon38.png"
        },
        "default_title":"verificador de avaliações",
        "default_popup":"index.html"
    },
    "content_scripts": [
        {
            "js":[
                "index.js",
                "./src/main.js"
            ],
            "matches":[
                "*://*/*",
                "<all_urls>"
            ]
        }
    ]

}